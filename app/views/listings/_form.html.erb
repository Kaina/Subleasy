<%= form_for @listing do |f| %>
  <%= f.date_select("start_date", start_year: Time.now.year, end_year: Time.now.year + 1, :order => [:month, :day, :year]) %>
  <%= f.date_select("end_date", start_year: Time.now.year, end_year: Time.now.year + 3, :order => [:month, :day, :year]) %>
  <%= f.text_field :address %>
  <%= f.text_field :city %>
  <%= f.text_field :state %>
  <%= f.text_field :zip %>
  <%= f.text_field :apartment_type %>
  <%= f.text_field :price_low%>
  <%= f.text_field :price_high %>
  <%= f.text_field :description %>
  <%= f.text_field :bedrooms %>
  <%= f.text_field :bathrooms %>
  <%= f.text_field :square_footage %>
  <%= f.submit "Post Listing" %>
<% end %>
